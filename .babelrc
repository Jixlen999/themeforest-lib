{
  "plugins": [
    [
      "babel-plugin-styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@src": ["./src"],
          "@components": ["./src/components"],
          "@utils": ["./src/utils"]
        }
      }
    ]
  ],
  "presets": ["@babel/preset-env", "@babel/preset-react"]
}
